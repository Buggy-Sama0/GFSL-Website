{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js",
      "headers": {
        "Access-Control-Allow-Origin": "https://gfsl-website.vercel.app",
        "Access-Control-Allow-Credentials": "true"
      }

    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}